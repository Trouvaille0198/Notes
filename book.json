{
    "plugins": [
        "-lunr",
        "-search",
        "highlight",
        "search-pro",
        "latex-codecogs",
        "expandable-chapters",
        "mermaid",
        "anchor-navigation-ex",
        "page-footer-ex"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": false,
            "mode": "float",
            "showGoTop": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "page-footer-ex": {
            "copyright": "By <em>MelonSoul</em>",
            "markdown": false,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}